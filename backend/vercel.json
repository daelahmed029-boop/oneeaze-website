{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@oneeaze-mongodb-uri",
    "JWT_SECRET": "@oneeaze-jwt-secret",
    "EMAIL_SERVICE": "@oneeaze-email-service",
    "EMAIL_USER": "@oneeaze-email-user",
    "EMAIL_PASS": "@oneeaze-email-pass"
  }
}
